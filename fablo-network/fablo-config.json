{
  "$schema": "https://github.com/softwaremill/fablo/releases/download/1.0.2/schema.json",
  "global": {
    "fabricVersion": "2.4.0",
    "tls": true
  },
  "orgs": [
    {
      "organization": {
        "name": "Orderer",
        "domain": "orderer.example.com"
      },
      "orderers": [
        {
          "groupName": "group1",
          "type": "solo",
          "instances": 1
        }
      ],
      "tools": {}
    },
    {
      "organization": {
        "name": "FarmerOrg",
        "domain": "farmer.example.com"
      },
      "peer": {
        "instances": 2,
        "db": "CouchDb"
      },
      "tools": {}
    },
    {
      "organization": {
        "name": "AuditorOrg",
        "domain": "auditor.example.com"
      },
      "peer": {
        "instances": 2,
        "db": "CouchDb"
      },
      "tools": {}
    },
    {
      "organization": {
        "name": "TransporterOrg",
        "domain": "transporter.example.com"
      },
      "peer": {
        "instances": 2,
        "db": "CouchDb"
      },
      "tools": {}
    }
  ],
  "channels": [
    {
      "name": "trackmeat-channel",
      "orgs": [
        {
          "name": "FarmerOrg",
          "peers": ["peer0", "peer1"]
        },
        {
          "name": "AuditorOrg",
          "peers": ["peer0", "peer1"]
        },
        {
          "name": "TransporterOrg",
          "peers": ["peer0", "peer1"]
        }
      ]
    }
  ],
  "chaincodes": []
}
